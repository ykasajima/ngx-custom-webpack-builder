{
  "$schema": "@angular-devkit/architect/src/builders-schema.json",
  "builders": {
    "browser": {
      "class": "./dist/browser",
      "schema": "./dist/browser/schema.json",
      "description": "Build a browser app."
    },
    "dev-server": {
      "class": "./dist/dev-server",
      "schema": "./dist/dev-server/schema.json",
      "description": "Serve a browser app."
    },
    "karma": {
      "class": "./dist/karma",
      "schema": "./dist/karma/schema.json",
      "description": "Run Karma unit tests."
    },
    "server": {
      "class": "./dist/server",
      "schema": "./dist/server/schema.json",
      "description": "Build a server Angular application."
    }
  }
}
